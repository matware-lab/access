{
    "name": "joomla/access",
    "type": "joomla-package",
    "description": "Joomla Access Package",
    "keywords": ["joomla", "framework", "access"],
    "homepage": "https://github.com/matware-lab/access",
    "license": "GPL-2.0+",
    "require": {
        "php": "^5.3.10|~7.0",
        "joomla/compat": "~1.0",
        "joomla/utilities": "~1.0",
				"joomla/di": "~1.3"
    },
    "require-dev": {
        "symfony/yaml": "~2.0|~3.0",
        "joomla/test": "~1.0",
        "phpunit/phpunit": "~4.8|~5.0",
        "squizlabs/php_codesniffer": "1.*"
    },
    "suggest": {
        "symfony/yaml": "Install symfony/yaml if you require YAML support."
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Access\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
